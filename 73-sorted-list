public boolean isSorted(List<Integer> numbers) {
    //code here
    boolean sorted = false;
    list<Integer> newlist = new list<Integer>();
    newlist = numbers;
    numbers.sort();
    if(newlist==numbers)
        sorted = true;
    else{
        list<Integer> l = new list<Integer>();
        for(Integer i=newlist.size()-1;i>=0;i--){
            l.add(newlist[i]);
        }
        if(newlist==l)
           sorted = true;
    }
    return sorted;
}