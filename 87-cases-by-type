public Map<String, List<Case>> casesByType(List<Case> cases) {
    //code here
    Map<String,List<Case>> mp = new Map<String,List<Case>>();
    set<String> s = new set<String>();
    for(Case c:cases)
    {
        List<Case> cl = new List<Case>();
        if(s.contains(c.Type))
        {
           cl = mp.get(c.Type);
           mp.put(c.Type, cl.add(c));
        }
        else
        {
            mp.put(c.Type,c);
            s.add(c.Type);
        } 
    }
    return mp;
}