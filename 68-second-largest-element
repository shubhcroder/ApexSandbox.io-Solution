public Integer secondLargest(List<Integer> numbers) {
    //code here
    map<Integer,Integer> m = new map<Integer,Integer>();
    numbers.sort();
    for(Integer i:numbers){
        if(!m.containsKey(i)){
            m.put(i,i);
        }
    }
    numbers.clear();
    numbers = m.values();
    return numbers.get(numbers.size()-2);
}